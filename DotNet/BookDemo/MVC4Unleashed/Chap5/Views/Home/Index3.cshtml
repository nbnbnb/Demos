@{
    Layout = null;
}

@using Chap5.Models
@model CommonHttpHeaders

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>HTTP 报表列表</title>
</head>
<body>
    <table>
       <tr>
           <th>Name</th>
           <th>Value</th>
       </tr>
        <tr>
            <td>Accept</td>
            <td>@Model.Accept</td>
        </tr>
        <tr>
            <td>CacheControl</td>
            <td>@Model.CacheControl</td>
        </tr>
        <tr>
            <td>AcceptEncoding</td>
            <td>@Model.AcceptEncoding</td>
        </tr>
        <tr>
            <td>AcceptLanguage</td>
            <td>@Model.AcceptLanguage</td>
        </tr>
        <tr>
            <td>Connection</td>
            <td>@Model.Connection</td>
        </tr>
        <tr>
            <td>Host</td>
            <td>@Model.Host</td>
        </tr>
        <tr>
            <td>UserAgent</td>
            <td>@Model.UserAgent</td>
        </tr>
    </table>
</body>
</html>
